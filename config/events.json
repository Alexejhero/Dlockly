{
  "channelCreate": {
    "parameters": [
      "channel"
    ],
    "check": "channel.guild",
    "guildGetter": "channel.guild"
  },
  "channelDelete": {
    "parameters": [
      "channel"
    ],
    "check": "channel.guild",
    "guildGetter": "channel.guild"
  },
  "emojiCreate": {
    "parameters": [
      "emoji"
    ],
    "check": "emoji.guild",
    "guildGetter": "emoji.guild"
  },
  "emojiDelete": {
    "parameters": [
      "emoji"
    ],
    "check": "emoji.guild",
    "guildGetter": "emoji.guild"
  },
  "guildMemberAdd": {
    "parameters": [
      "member"
    ],
    "check": "member.guild",
    "guildGetter": "member.guild"
  },
  "guildMemberRemove": {
    "parameters": [
      "member"
    ],
    "check": "member.guild",
    "guildGetter": "member.guild"
  },
  "guildMemberUpdate": {
    "parameters": [
      "oldmember",
      "member"
    ],
    "check": "member.guild && oldmember.nickname != member.nickname",
    "guildGetter": "member.guild"
  },
  "message": {
    "parameters": [
      "message"
    ],
    "check": "message.guild && !message.author.bot",
    "guildGetter": "message.guild"
  },
  "messageDelete": {
    "parameters": [
      "message"
    ],
    "check": "message.guild && !message.author.bot",
    "guildGetter": "message.guild"
  },
  "messageUpdate": {
    "parameters": [
      "oldmessage",
      "message"
    ],
    "check": "message.guild && !message.author.bot && oldmessage.content != message.content",
    "guildGetter": "message.guild"
  }
}